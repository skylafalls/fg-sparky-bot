{
  "name": "fg-sparky-bot",
  "description": "Source code for the FG sparky bot, inspired by the GD discord sparky boy",
  "private": true,
  "packageManager": "bun@1.3.1",
  "license": "GPL-3.0-or-later",
  "version": "0.0.0",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "start": "bun run dist/index.js",
    "build": "bun build src/index.ts --target=bun --production --outdir=dist --sourcemap=linked",
    "lint": "oxlint . --ignore-path=.gitignore && eslint .",
    "lint:fix": "oxlint --fix --fix-suggestions . --ignore-path=.gitignore && eslint --fix .",
    "typecheck": "tsc -p tsconfig.json"
  },
  "dependencies": {
    "@discordjs/voice": "^0.19.0",
    "bufferutil": "^4.0.9",
    "discord.js": "^14.24.2"
  },
  "devDependencies": {
    "@js-temporal/polyfill": "^0.5.1",
    "@stylistic/eslint-plugin": "^5.5.0",
    "@types/bun": "^1.3.2",
    "chalk": "^5.6.2",
    "eslint": "^9.39.1",
    "eslint-plugin-oxlint": "^1.26.0",
    "jiti": "^2.6.1",
    "oxlint": "^1.26.0",
    "typescript-eslint": "^8.46.3"
  }
}